      <Button fx:id="back" layoutX="988.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="34.0" prefWidth="78.0" text="Retour" />
      <TableView fx:id="tableShop" layoutX="14.0" layoutY="283.0" prefHeight="423.0" prefWidth="1051.0">
