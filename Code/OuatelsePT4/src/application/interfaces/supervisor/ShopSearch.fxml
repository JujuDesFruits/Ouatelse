            <TableColumn fx:id="shopAddress" prefWidth="457.0" text="Adresse" />
          <TableColumn fx:id="cityName" prefWidth="219.0" text="Ville" />
          <TableColumn fx:id="managerName" prefWidth="374.0" text="Responsable" />
        </columns>
      </TableView>
   </children>
